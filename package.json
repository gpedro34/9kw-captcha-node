{
  "name": "9kw-captcha-node",
  "version": "1.0.0",
  "description": "9kw API Wrapper to solve captchas",
  "main": "src/index.js",
  "scripts": {
    "flow": "npm run lint && npm run docs",
    "docs": "npm run build:docs && npm run move:docs",
    "build:docs": "./node_modules/.bin/jsdoc -c jsdoc.json || true",
    "move:docs": "cp -r ./docs/9kw-captcha-node/1.0.0/* ./docs/ && rm -r ./docs/9kw-captcha-node",
    "serve:docs": "serve docs",
    "lint": "eslint src/. test/. --config .eslintrc.json --fix",
    "test": "mocha test/ -t 0 --check-leaks --exit --inline-diffs --recursive"
  },
  "keywords": [
    "9kw",
    "recaptcha",
    "reCaptchaV3",
    "reCaptchaV2",
    "captchaImage",
    "captchaBase64",
    "captchaURL",
    "captchaSiteKey",
    "captcha"
  ],
  "author": "gpedro34",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gpedro34/9kw-captcha-node.git"
  },
  "bugs": {
    "url": "https://github.com/gpedro34/9kw-captcha-node/issues"
  },
  "dependencies": {
    "superagent": "^2.3.0"
  },
  "devDependencies": {
    "better-docs": "^2.3.2",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "jsdoc": "^3.6.5",
    "jsdoc-mermaid": "^1.0.0",
    "mocha": "^8.1.3",
    "nock": "^13.0.4",
    "prettier": "^2.1.1",
    "serve": "^11.3.2"
  }
}
